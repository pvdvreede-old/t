{% extends 'VdvreedeTFrontendBundle::layout.html.twig' %}
{% block content %}

<form action="{{ path('import_process', { 'accountId' : accountId }) }}" method="post" {{ form_enctype(form) }}>

    {{ form_widget(form) }}

    <table>
    {% for lines in firstLines %}
    <tr>
    {% for column in lines %}
    <td>{{column}}</td>
    {% endfor %}
    </tr>
    {% endfor %}
    </table>
    {{ form_rest(form) }}
    <input type="submit" value="Next" />
</form>

{% endblock %}