<div class="paginator">
    <ul>
        {% if paginator.currentpage != 1 %}
            <li> <a class="previous" href="{{ path(item_type, {  'offset': paginator.currentpage-1 }) }}">Previous</a>

        {% endif %}
        {% for i in 1..paginator.numpages%}

        {% if paginator.range.0 > 2 and i == paginator.range.0 %}
              ...
        {% endif %}


          {%  if(i==1 or i==paginator.numpages or i in paginator.range) %}

            {% if i==paginator.currentpage %}
            <li><a class="active" href="{{ path(item_type, {  'offset': i })}}">{{i}}</a></li>
            {% else %}
            <li><a href="{{ path(item_type, {  'offset': i }) }}"> {{i}}</a></li>
            {% endif %}
          {% endif %}

         {% if paginator.range[paginator.midrange -1] < paginator.numpages -1 and i == paginator.range[paginator.midrange-1] %}
              ...
         {% endif %}

        {% endfor %}

        {% if paginator.numpages > 1 %}
            <li> <a class="next" href="{{ path(item_type, {  'offset': paginator.currentpage+1 }) }}">Next</a>
        {% endif %}
    </ul>
</div>
